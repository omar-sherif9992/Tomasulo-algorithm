{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["es2017", "esnext.asynciterable"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "outDir": "dist",
  "jsx":"react-jsx",
    "paths": {
      "@/*": ["*"],
      "@assembly/*": ["./assembly/*"],
      "@components/*": ["./components/*"],
      "@logic/*": ["./logic/*"],
      "@Common/*": ["./Common/*"],
      "@Config": ["./Config"],
    },
    "pretty": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "target": "es2017",
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["node_modules", "src/http", "src/logs"],
  "include": ["src/**/*.ts", "src/**/*.json", ".env", "swagger.ts", "swagger.ts", "src/logic/Main.tsx", "src/logic/Main.tsx", "src/logic/Arrays.tsx", "src/logic/Arrays.tsx", "src/logic/Issuer.tsx", "src/logic/Issuer.tsx", "src/logic/Parser.tsx", "src/logic/Parser.tsx"]
}
